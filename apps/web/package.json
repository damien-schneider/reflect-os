    "dev:zero-cache": "dotenv -e ../../.env -- zero-cache-dev",
    "dev:react-grab": "npx @react-grab/claude-code@latest",
    "kill:react-grab": "pkill -f 'react-grab.*server' || true",
    "build": "vite build",
    "typecheck": "tsc -b",
    "check-types": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
