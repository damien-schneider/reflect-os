{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./src/components/*.tsx"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "import": "./src/hooks/*.ts"
    },
    "./lib/*": {
      "types": "./src/lib/*.ts",
      "import": "./src/lib/*.ts"
    },
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^4.0.8",
    "recharts": "2.15.4",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  }
}
